rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false;
    }
    match /test/{docId} {
     allow read: if true;
     allow write: if true;
    }
    match /salas/{docId} {
     allow read: if true;
     allow write: if true;
    }
    match /preguntas/{docId} {
     allow read: if true;
     allow write: if true;
    }
    match /usuarios/{docId} {
     allow read: if true;
     allow write: if true;
    }
  }
}